from fastapi import FastAPI ,UploadFile , File
import uvicorn
import tensorflow as tf


app = FastAPI()



@app.get("/ping")
def ping():
  return "I am alive"

@app.post("/predict")
def predict(
        file: UploadFile = File(...)
):



  return file.filename




if __name__ == '__main__':
  uvicorn.run(app , host = 'localhost' , port = 8000)